{
  "name": "aiana-player-reloaded",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/storybook-addon-jsx": "^5.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "focus-visible": "^4.1.5",
    "react": "^16.4.2",
    "react-docgen-typescript-loader": "^2.2.0",
    "react-dom": "^16.4.2",
    "react-intl": "^2.4.0",
    "react-scripts-ts": "2.17.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/styles --include-path ./node_modules src/styles -o src/",
    "watch-css": "npm run build-css && node-sass  --include-path ./src/styles --include-path ./node_modules src/styles -o src/ --watch",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-svg": "svgr --template ./.svgr/tsxTemplate.js --ext tsx -d ./src/components/svg/ ./src/svg/",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-svg build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.2",
    "@types/react": "^16.4.12",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.8",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__react": "^3.0.8",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.3.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "storybook-addon-intl": "^2.3.0",
    "storybook-addon-jsx": "^5.3.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^3.5",
    "typescript": "^3.0.1",
    "webpack": "^3.12.0"
  },
  "optionalDependencies": {
    "@svgr/cli": "^2.2.0",
    "prettier": "^1.14.2"
  }
}
