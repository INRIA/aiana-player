{
  "name": "aiana-player-reloaded",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,md}": [
      "prettier --write --config .prettierrc",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "color-convert": "^1.9.3",
    "focus-visible": "^4.1.5",
    "i18next": "^11.8.0",
    "marked": "^0.5.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-i18next": "^7.12.0",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.5"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "svg": "svgr --template ./.svgr/tsxTemplate.js --icon --ext tsx -d ./src/components/svg/ ./svg/",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@svgr/cli": "^2.4.1",
    "@types/classnames": "^2.2.6",
    "@types/color-convert": "^1.9.0",
    "@types/i18next": "^8.4.5",
    "@types/jest": "^23.3.2",
    "@types/marked": "^0.4.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-i18next": "^7.8.2",
    "@types/react-redux": "^6.0.6",
    "@types/redux-logger": "^3.0.6",
    "@types/storybook-addon-jsx": "^5.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^3.0.8",
    "awesome-typescript-loader": "^5.2.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react-docgen-typescript-loader": "^2.2.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "redux-devtools-extension": "^2.13.5",
    "storybook-addon-jsx": "^5.3.0",
    "ts-loader": "^3.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.1.3",
    "webpack": "^3.12.0"
  }
}
